{
  "name": "@pi-base/core",
  "type": "module",
  "version": "0.4.0",
  "description": "Shared pi-base data models",
  "keywords": [
    "Node",
    "Math",
    "Topology"
  ],
  "homepage": "https://topology.pi-base.org",
  "bugs": {
    "url": "https://github.com/pi-base/web/issues"
  },
  "license": "MIT",
  "author": "James Dabbs <james.dabbs@gmail.com> (https://jdabbs.com)",
  "types": "./dist/types/index.d.ts",
  "exports": {
    "types": "./dist/types/index.d.ts",
    "require": "./dist/cjs/index.js",
    "import": "./dist/esm/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pi-base/web.git"
  },
  "scripts": {
    "build:peg": "peggy --plugin ./node_modules/ts-pegjs/dist/tspegjs -o src/Formula/Grammar.ts --cache src/Formula/Grammar.pegjs",
    "build": "pnpm build:peg && ./bin/build",
    "dev": "pnpm build:peg && tsc --watch",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "micromark-util-types": "1.0.2",
    "rehype-katex": "^6.0.3",
    "rehype-stringify": "^9.0.4",
    "remark": "^14.0.3",
    "remark-math": "^5.1.1",
    "remark-parse": "^10.0.2",
    "remark-rehype": "^10.1.0",
    "unified": "^10.1.2",
    "unist-util-is": "^5.2.1",
    "unist-util-visit": "^4.1.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/hast": "^2.3.9",
    "@types/mdast": "^3.0.15",
    "@types/unist": "^2.0.10",
    "hast-util-to-html": "^8.0.4",
    "mdast-util-from-markdown": "1.3.0",
    "peggy": "^3.0.2",
    "ts-pegjs": "^4.2.1"
  }
}
